{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "padded-blocks": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "no-underscore-dangle": 0,
    "no-nested-ternary": 0,
    "no-prototype-builtins": 0,
    "no-unused-vars": 0
  }
}
